
  <% if user_signed_in? && current_user.is_not_student? %>
    <% if current_user.is_admin? || current_user.is_operator? || current_user.is_proofreader? || current_user.is_teacher?%>
        <%= render 'aform' %>

    <% elsif current_user.is_teacher? %>
        <%= render 'tform' %>
    <% else %>
      <%= render 'usform' %>
    <%end %>

  <% else %>
    <%= render 'uform' %>
  <% end %>

