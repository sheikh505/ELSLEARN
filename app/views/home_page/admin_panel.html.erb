            <table class="table table-striped">
              <thead>
              <tr>
                <th>Questions</th>
                <th>Published</th>
                <th>Not-Published</th>
                <th>Total</th>
              </tr>
              </thead>
              <tbody>
              <tr>
                <td>MCQs</td>
                <td><%= @questions_mcq_publish %></td>
                <td><%= @questions_mcq_not_publish %></td>
                <td><%= @total_questions_mcq %></td>
              </tr>
              <tr>
                <td>True/False</td>
                <td><%= @questions_truefalse_publish %></td>
                <td><%= @questions_truefalse_not_publish %></td>
                <td><%= @total_questions_truefalse %></td>
              </tr>
              <tr>
                <td>Fill in the Blanks</td>
                <td><%= @questions_blank_publish %></td>
                <td><%= @questions_blank_not_publish %></td>
                <td><%= @total_questions_blank %></td>
              </tr>
              <tr>
                <td>Descriptive Questions</td>
                <td><%= @questions_descriptive_publish %></td>
                <td><%= @questions_descriptive_not_publish %></td>
                <td><%= @total_questions_descriptive %></td>
              </tr>
              <tr>
                <td>TOTAL</td>
                <td><%= @total_publish %></td>
                <td><%= @total_not_publish %></td>
                <td><%= @total_questions %></td>
              </tr>
              </tbody>
            </table>
          </div>
        </div>
      </div>

      <div class="col-lg-6">
        <div class="ibox float-e-margins">
          <div class="ibox-title">
            <h5>Total Questions </h5>

            <div ibox-tools></div>
          </div>
          <div class="ibox-content">

            <table class="table table-striped">
              <thead>
              <tr>
                <th>Questions</th>
                <th>Published</th>
                <th>Not-Published</th>
                <th>Total</th>
              </tr>
              </thead>
              <tbody>
              <tr>
                <td>MCQs</td>
                <td><%= @questions_mcq_publish %></td>
                <td><%= @questions_mcq_not_publish %></td>
                <td><%= @total_questions_mcq %></td>
              </tr>
              <tr>
                <td>True/False</td>
                <td><%= @questions_truefalse_publish %></td>
                <td><%= @questions_truefalse_not_publish %></td>
