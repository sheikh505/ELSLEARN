<% if current_user.is_admin? %>
<section class="dashboard-main">
  <div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-ld-12">
      <h1 class="title">Dashboard</h1>
    </div>
  </div>
  <div class="wrapper wrapper-content">

    <div class="row ">

      <div class="col-lg-6">
        <div class="ibox float-e-margins">
